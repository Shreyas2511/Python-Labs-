#Functions to check if the given no is prime or not
def check_prime(num):
  if num >1:
    for i in range (2,num):
      if num %i == 0 :
        print(f"{num}is not a prime number")
        break
    else:
        print(f"{num} is a prime number")
  else:
    print(f"{num} is not prime number")
number = int(input("Enter a NUmber :"))
check_prime(number)
