# Object oriented programing
class dog:
  def __init__(self,name,age):
    self.name = name
    self.age = age
  def bark(self):
    return f"{self.name} is barking"
my_dog=dog("leo",3)
print(my_dog.name)
print(my_dog.age)
print(my_dog.bark())

class person:
  def __init__(self,name,age):
    self.name=name
    self.age=age
  def greet(self):
    return f"Hello , my name is {self.name} and I am {self.age} years ok"
person1 = person("Alice",80)
print(person1.name)
print(person1.age)
print(person1.greet())
