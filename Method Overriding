#Method Overriding
class animal():
  def __init__(self,name):
    self.name = name
  def speak(self):
    print(f"{self.name} makes a sound")
class dog(animal):
  def __init__(self,name):
    super().__init__(name)
  def speak(self):
    print(f"{self.name}barks")
a = animal("Genetic Animal")
a.speak()
d= dog("Buddy")
d.speak() 
